  <sections>
    <section code="1" name="Раздел А" nb="1">
      <columns>
        <column code="1" type="B" name="Наименование показателя">
          <default-cell column="1" format="C(250)" inputType="0" />
        </column>
        <column code="2" type="B" name="№ строки">
          <default-cell column="2" format="C(5)" inputType="0" />
        </column>
        <column code="3" type="V" name="Ед.изм.">
          <default-cell column="3" format="C(8)" inputType="0" />
        </column>
        <column code="4" type="Z" name="За отчетный год">
          <default-cell column="4" format="N(15,1)" inputType="2" />
        </column>
      </columns>
      <rows>
        <row code="01" type="F" name="Численность пострадавших при несчастных случаях на производстве с утратой трудоспособности на 1 рабочий день и более и со смертельным исходом">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="02" type="F" name="   из них: женщин">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="03" type="F" name="               лиц до 18 лет">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="04" type="F" name="               иностранных граждан">
          <cell column="3" format="C(8)" default="чел." inputType="2" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="05" type="F" name="Из стр.01 численность пострадавших со смертельным исходом">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="06" type="F" name="   из них: женщин">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="07" type="F" name="               лиц до 18 лет">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="08" type="F" name="               иностранных граждан">
          <cell column="3" format="C(8)" default="чел." inputType="2" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="09" type="F" name="Число человеко-дней нетрудоспособности у пострадавших с утратой трудоспособности на 1 рабочий день и более, временная нетрудоспособность которых закончилась в отчетном году">
          <cell column="3" format="C(8)" default="чел.-дн." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="10" type="F" name="Численность пострадавших частично утративших трудоспособность и переведенных с основной работы на другую на 1 рабочий день и более в соответствии с медицинским заключением">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="11" type="F" name="   из них: женщин">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="12" type="F" name="Численность лиц с впервые установленным профессиональным заболеванием">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="13" type="F" name="Израсходовано на мероприятия по охране труда - всего">
          <cell column="3" format="C(8)" default="тыс.руб." inputType="0" />
          <cell column="4" format="N(15,1)" inputType="2" />
        </row>
        <row code="" type="C" name="Справочно:" />
        <row code="14" type="F" name="Средняя численность работников (работники списочного состава и внешние совместители) за отчетный год">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="1" />
        </row>
        <row code="15" type="F" name="     из них: женщин">
          <cell column="3" format="C(8)" default="чел." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="2" />
        </row>
        <row code="16" type="F" name="Наличие на предприятии здравпункта (врачебного кабинета, медико-санитарной части и т.п.) 1-ДА, 0 - НЕТ">
          <cell column="3" format="C(8)" default="ед." inputType="0" />
          <cell column="4" format="N(15,0)" inputType="1" vldType="3" vld="0,1" />
        </row>
      </rows>
    </section>
